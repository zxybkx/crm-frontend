{
  "name": "crm-frontend",
  "version": "1.0.8",
  "description": "An out-of-box UI solution for enterprise applications",
  "private": true,
  "main": "config/config.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development UMI_ENV=dev PORT=9007 APP_TYPE=site umi dev",
    "build": "cross-env NODE_ENV=production UMI_ENV=prod umi build",
    "site": "npm run presite && cross-env APP_TYPE=site npm run build && firebase deploy && npm run docker:push",
    "analyze": "cross-env ANALYZE=1 umi build",
    "lint:prettier": "check-prettier lint",
    "lint": "eslint --ext .js src mock tests && npm run lint:style && npm run lint:prettier",
    "lint:fix": "eslint --fix --ext .js src mock tests && stylelint --fix 'src/**/*.less' --syntax less",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js",
    "test": "umi test",
    "test:component": "umi test ./src/lib",
    "test:all": "node ./tests/run-tests.js",
    "prettier": "node ./scripts/prettier.js",
    "docker:dev": "docker-compose -f ./docker/docker-compose.dev.yml up",
    "docker:build": "docker-compose -f ./docker/docker-compose.dev.yml build",
    "docker-prod:dev": "docker-compose -f ./docker/docker-compose.yml up",
    "docker-prod:build": "docker-compose -f ./docker/docker-compose.yml build",
    "docker-hub:build": "docker build  -f Dockerfile.hub -t  ant-design-pro ./",
    "docker:tag": "docker tag ant-design-pro antdesign/ant-design-pro",
    "docker:push": "npm run docker-hub:build && npm run docker:tag && docker push antdesign/ant-design-pro"
  },
  "dependencies": {
    "casic-common": "^1.2.3",
    "echarts": "^4.3.0",
    "echarts-for-react": "^2.0.15-beta.0",
    "pubsub-js": "^1.7.0"
  },
  "devDependencies": {
    "@antv/data-set": "^0.10.2",
    "@antv/g6": "^3.0.4",
    "@babel/runtime": "^7.5.5",
    "@types/history": "^4.7.2",
    "@types/react": "16.9.0",
    "@types/react-dom": "16.9.0",
    "antd": "^3.20.5",
    "antd-pro-merge-less": "^1.0.0",
    "antd-theme-webpack-plugin": "^1.3.0",
    "babel-eslint": "^10.0.2",
    "chalk": "^2.4.2",
    "check-prettier": "^1.0.3",
    "classnames": "^2.2.6",
    "cross-env": "^5.2.0",
    "cross-port-killer": "^1.1.1",
    "enquire-js": "^0.2.1",
    "enzyme": "^3.10.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-compat": "^3.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-react": "^7.14.3",
    "gg-editor": "^2.0.4",
    "hash.js": "^1.1.7",
    "husky": "^3.0.1",
    "js-cookie": "^2.2.0",
    "jsencrypt": "^3.0.0-rc.1",
    "jsonwebtoken": "^8.5.1",
    "less": "^3.9.0",
    "lint-staged": "^9.2.0",
    "lodash": "^4.17.15",
    "lodash-decorators": "^6.0.1",
    "memoize-one": "^5.0.5",
    "moment": "^2.24.0",
    "numeral": "^2.0.6",
    "nzh": "^1.0.4",
    "omit.js": "^1.0.2",
    "path-to-regexp": "^3.0.0",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2",
    "qs": "^6.7.0",
    "rc-animate": "^2.9.2",
    "react": "^16.8.6",
    "react-container-query": "^0.11.0",
    "react-contexify": "^4.1.1",
    "react-copy-to-clipboard": "^5.0.1",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.8.6",
    "react-fittext": "^1.0.0",
    "react-media": "^1.9.2",
    "react-new-window": "^0.1.2",
    "react-sortable-hoc": "^1.9.1",
    "react-table": "^6.10.0",
    "react-table-hoc-fixed-columns": "^2.1.1",
    "react-text-mask": "^5.4.3",
    "slash2": "^2.0.0",
    "store": "^2.0.12",
    "umi": "^2.8.15",
    "umi-plugin-authorize": "^1.0.3",
    "umi-plugin-react": "^1.9.13",
    "ztree-for-react": "^1.2.0"
  },
  "lint-staged": {
    "**/*.{js,ts,tsx,json,jsx,less}": [
      "node ./scripts/lint-prettier.js",
      "git add"
    ],
    "**/*.{js,jsx}": "npm run lint-staged:js",
    "**/*.less": "stylelint --syntax less"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ]
}
